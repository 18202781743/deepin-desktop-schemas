<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema path="/com/deepin/dde/debug/" id="com.deepin.dde.debug" gettext-domain="DDE">
    <key type="i" name="cpu-percentage">
      <range min="50" max="100" />
      <default>80</default>
      <summary>Current cpu usage rate</summary>
      <description>
        The current cpu percentage.
      </description>
    </key>
    <key type="i" name="mem-usage">
      <range min="50" max="1000" />
      <default>100</default>
      <summary>Current memory usage</summary>
      <description>
        The current allocated memory.
      </description>
    </key>
    <key type="i" name="duration">
      <range min="1" max="100" />
      <default>5</default>
      <summary>Duration Seconds</summary>
      <description>
        The loop duration seconds.
      </description>
    </key>
  </schema>
</schemalist>
